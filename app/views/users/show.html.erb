<p><strong>Raffle History:</strong></p>
<% @user.raffles.uniq.each do |raffle| %>
 <% if raffle.winner == @user.username%>
        <p>WON</p>
        <% else %>
        <p>LOST</p>
            <% end %>
        <p><%= raffle.product_name %></p>
        <br>
        <p><img src="<%= raffle.product_image%>"></p>
        <br>
       
    <% end %>